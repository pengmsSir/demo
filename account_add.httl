    function onReset(){
        form.reset();
        form.setChanged(false);
    }
    
    function onCancel(){
        CloseWindow("cancel");
    }
    
    function CloseWindow(action){
        if(action=="close" && form.isChanged()){
            if(confirm("数据已改变,是否确认关闭?")){
                return window.close();
            }else{
                return false;
            }
        }else if(window.CloseOwnerWindow){
            return window.CloseOwnerWindow(action);
        }else{
            return window.close();
        }
    }




    function checkInputProjectName()
    {
        var projectName = nui.get("projectId").text;
        if(projectName=="")
        {
            return;
        }
        var obj={"pjName":projectName};
        var json = nui.encode(obj);
        $.ajax({
            url:"${basePath}/pms/queryProjectIdByName",
            type:'POST',
            data:json,
            cache:false,
            contentType:'application/json',
            success:function(text){
                var returnJson =text;
                if(returnJson.status == true){
                    nui.get("projectId").setValue(returnJson.projectId);
                }else{
                    nui.alert("输入的项目名称不存在");
                    nui.get("projectId").setText("");
                }
            }
        });
    }



