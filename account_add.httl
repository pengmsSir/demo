    function frAndSf(){
        var fr=nui.get("acc.isOpenFR");
        var sf=nui.get("acc.isOpenSF");


        if((fr.getChecked()&&sf.getChecked())||(!fr.enabled&&sf.getChecked())||(fr.getChecked()&&!sf.enabled)){
            if(fr.checked==true){
                fr.setChecked(!fr.getChecked());
            }
            if(sf.checked==true){
                sf.setChecked(!sf.getChecked());
            }
            nui.alert("券商模式(三方存管账户)和法人结算(席位租用)不能同时开立");
            return;
        }
    }
    function submitData(){
        if(!(nui.get("id_open_tg").getChecked()||nui.get("acc.isOpenFR").getChecked()||nui.get("acc.isOpenSF").getChecked()||
                nui.get("id_open_zqsh").getChecked()||nui.get("id_open_zqsz").getChecked()||nui.get("id_open_zqbj").getChecked()||
                nui.get("id_open_gzqh").getChecked()||nui.get("id_open_yhjzy").getChecked()||nui.get("id_open_yhjsh").getChecked())){
            nui.alert("请选择一种账户种类");
            return;
        }

        if(nui.get("id_open_tg").getChecked() && nui.get("accountBank").getValue ==null ){
            nui.alert("开户行不能为空");
            return;
        }

        form.validate();
        if(form.isValid()==false) return;
        var data = form.getData(true,true);
        data.acc.notes = accnotesEditor.html();
        data.acc.projectName = nui.get("projectId").text;
        data.acc.accountBankName = nui.get("accountBank").text;
        data.acc.docId=nui.get("hidden_doc_id").getValue();
        var json = nui.encode(data.acc);
        var messageid = nui.loading("数据提交中，请稍后...","提交中");
        var json = nui.encode(data.acc);
        $.ajax({
            url:"${basePath}/acc/submitAccountApplyInfo",
            type:'POST',
            data:json,
            cache:false,
            contentType:'application/json',
            success:function(text){
                nui.hideMessageBox(messageid);
                var returnJson =text;
                if(returnJson.status == true){
                    if(returnJson.userCheckResult==null){
                        nui.alert( "提交账户开户申请信息成功", "系统提示", function (action) {
                            CloseWindow("saveSuccess");
                        });
                    }
                    else{
                        nui.alert(returnJson.userCheckResult,"系统提示", function (action) {
                            if (action == "ok" || action == "close") {
                                return;
                            }
                        });
                    }
                }else{
                    nui.alert("提交账户开户申请信息失败", "系统提示", function(action){
                        if(action == "ok" || action == "close"){
                            return;
                        }
                    });
                }
            }
        });
    }

    function saveData(){
        if(nui.get("id_open_tg").getChecked() && nui.get("accountBank").getValue ==null ){
            nui.alert("开户行不能为空");
            return;
        }
           form.validate();
        if(form.isValid()==false) return;
        var data = form.getData(true,true);
        data.acc.notes = accnotesEditor.html();
        data.acc.projectName = nui.get("projectId").text;
        data.acc.accountBankName = nui.get("accountBank").text;
        data.acc.docId=nui.get("hidden_doc_id").getValue();
        var json = nui.encode(data.acc);
        var messageid = nui.loading("数据保存中，请稍后...","保存中");
        $.ajax({
            url:"${basePath}/acc/saveAccountApplyInfo",
            type:'POST',
            data:json,
            cache:false,
            contentType:'application/json',
            success:function(text){
                nui.hideMessageBox(messageid);
                var returnJson =text;
			    if(returnJson.status == true){
                    nui.alert("新增账户申请信息成功", "系统提示", function(action){
                        CloseWindow("saveSuccess");
                    });
			    }else{
				    nui.alert("新增账户申请信息失败", "系统提示", function(action){
					    if(action == "ok" || action == "close"){
						    return;
					    }
				    });
			    }
            }
        });
    }

 /*   function setAccName(){
